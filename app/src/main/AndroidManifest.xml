<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smdproject.easygrocery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".customer.Login"></activity>
        <activity android:name=".customer.Cart" />
        <activity android:name=".customer.SingleStore" />
        <activity android:name=".customer.SingleStoreSubcategory" />
        <activity android:name=".MainActivity" />
        <activity android:name=".customer.Splash" />
        <activity android:name=".customer.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>